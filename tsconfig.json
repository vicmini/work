{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es2017",
    "module": "commonjs",
    "sourceMap": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["es2015","dom"],
    "typeRoots": ["./node_modules/@types"],
  },
  "include": ["serve/**/*"],
  "exclude": ["node_modules"]
}